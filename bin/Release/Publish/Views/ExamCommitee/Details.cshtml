@model IUMS.Entities.ACA_EXAM_COMMITTEE
@{
	Layout = null;
}

<div class="modal" id="modal" tabindex="-1" role="dialog">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<div class="col-md-10">
					<h3>Details - 
<span class="text-info pull-right">(PK) @Model.ID</span>
					</h3>
				</div>
				<div class="col-md-2">
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
				</div>
			</div>
			<div class="modal-body">
				<table class="table table-striped table-hover table-bordered">
					<thead class="thead-dark">
						<tr>
							<th scope="col">DisplayNameFor</th>
							<th scope="col">DisplayFor</th>
						</tr>
					</thead>
					<tbody>
						<tr>
							<td>@Html.DisplayNameFor(model => model.ID)</td>
							<td>@Html.DisplayFor(model => model.ID)</td>
						</tr>
						<tr>
							<td>@Html.DisplayNameFor(model => model.STATUS)</td>
							<td>@Html.DisplayFor(model => model.STATUS)</td>
						</tr>
						<tr>
							<td>@Html.DisplayNameFor(model => model.COMMITTEE_HEAD)</td>
							<td>@Html.DisplayFor(model => model.COMMITTEE_HEAD)</td>
						</tr>
						<tr>
							<td>@Html.DisplayNameFor(model => model.PROGRAM_ID)</td>
							<td>@Html.DisplayFor(model => model.PROGRAM_ID)</td>
						</tr>
						<tr>
							<td>@Html.DisplayNameFor(model => model.SEMESTER_ID)</td>
							<td>@Html.DisplayFor(model => model.SEMESTER_ID)</td>
						</tr>
						<tr>
							<td>@Html.DisplayNameFor(model => model.YEAR)</td>
							<td>@Html.DisplayFor(model => model.YEAR)</td>
						</tr>
						<tr>
							<td>@Html.DisplayNameFor(model => model.MEMBER1)</td>
							<td>@Html.DisplayFor(model => model.MEMBER1)</td>
						</tr>
						<tr>
							<td>@Html.DisplayNameFor(model => model.MEMBER2)</td>
							<td>@Html.DisplayFor(model => model.MEMBER2)</td>
						</tr>
						<tr>
							<td>@Html.DisplayNameFor(model => model.MEMBER3)</td>
							<td>@Html.DisplayFor(model => model.MEMBER3)</td>
						</tr>
						<tr>
							<td>@Html.DisplayNameFor(model => model.MEMBER4)</td>
							<td>@Html.DisplayFor(model => model.MEMBER4)</td>
						</tr>
						<tr>
							<td>@Html.DisplayNameFor(model => model.USER_ID)</td>
							<td>@Html.DisplayFor(model => model.USER_ID)</td>
						</tr>
						<tr>
							<td>@Html.DisplayNameFor(model => model.CREATE_DATE)</td>
							<td>@Html.DisplayFor(model => model.CREATE_DATE)</td>
						</tr>
					</tbody>
				</table>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-primary pull-right" data-dismiss="modal">Close</button>
			</div>
		</div>
	</div>
</div>
