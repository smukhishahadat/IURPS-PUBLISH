@model IUMS.Entities.ACADEMIC_STUDENT_PERSONAL
@{
	Layout = null;
}

<div class="modal" id="modal" tabindex="-1" role="dialog">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<div class="col-md-10">
					<h3>Student Details</h3>
				</div>
				<div class="col-md-2">
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
				</div>
			</div>
			<div class="modal-body">
				<table class="table table-striped table-hover table-bordered">
					
                    <tbody>

                        <tr>
                            <td>@Html.DisplayNameFor(model => model.STUDENT_ID)</td>
                            <td>@Html.DisplayFor(model => model.STUDENT_ID)</td>
                        </tr>
                        <tr>
                            <td>@Html.DisplayNameFor(model => model.ROLL)</td>
                            <td>@Html.DisplayFor(model => model.ROLL)</td>
                        </tr>
                        <tr>
                            <td>@Html.DisplayNameFor(model => model.REG_NO)</td>
                            <td>@Html.DisplayFor(model => model.REG_NO)</td>
                        </tr>
                        <tr>
                            <td>@Html.DisplayNameFor(model => model.FULL_NAME)</td>
                            <td>@Html.DisplayFor(model => model.FULL_NAME)</td>
                        </tr>

                        <tr>
                            <td>@Html.DisplayNameFor(model => model.MOBILE_NO)</td>
                            <td>@Html.DisplayFor(model => model.MOBILE_NO)</td>
                        </tr>

                        <tr>
                            <td>@Html.DisplayNameFor(model => model.EMAIL)</td>
                            <td>@Html.DisplayFor(model => model.EMAIL)</td>
                        </tr>
                        <tr>
                            <td>@Html.DisplayNameFor(model => model.SESSION_ID)</td>
                            <td>@Html.DisplayFor(model => model.SESSION_ID)</td>
                        </tr>
                        <tr>
                            <td>@Html.DisplayNameFor(model => model.FACULTY_ID)</td>
                            <td>@Html.DisplayFor(model => model.FACULTY_ID)</td>
                        </tr>
                        <tr>
                            <td>@Html.DisplayNameFor(model => model.DEPARTMENT_ID)</td>
                            <td>@Html.DisplayFor(model => model.DEPARTMENT_ID)</td>
                        </tr>
                        <tr>
                            <td>@Html.DisplayNameFor(model => model.PROGRAM_ID)</td>
                            <td>@Html.DisplayFor(model => model.PROGRAM_ID)</td>
                        </tr>
                        <tr>
                            <td>@Html.DisplayNameFor(model => model.YEAR)</td>
                            <td>@Html.DisplayFor(model => model.YEAR)</td>
                        </tr>

                        <tr>
                            <td>@Html.DisplayNameFor(model => model.STATUS)</td>
                            <td>@Html.DisplayFor(model => model.STATUS)</td>
                        </tr>

                        <tr>
                            <td>@Html.DisplayNameFor(model => model.HALL_ID)</td>
                            <td>@Html.DisplayFor(model => model.HALL_ID)</td>
                        </tr>

                    </tbody>
				</table>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-primary pull-right" data-dismiss="modal">Close</button>
			</div>
		</div>
	</div>
</div>
